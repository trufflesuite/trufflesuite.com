      <div class="row" id="isoContainer">
        {{#each tutorials as |tutorial slug|}}
          {{#if tutorial.published}}
            <div class="col-xs-12 col-md-6 col-lg-4 iso-item {{makeSlug tutorial.author}}">
              <a class="link-card" href="/tutorials/{{ slug }}">
                <div class="card card-tutorial {{listToClasses tutorial.products}}">
                  <div class="card-body">
                    <h5 class="card-title">{{ tutorial.title }}</h5>
                    <p class="card-text space-icon"><i class="far fa-calendar-alt"></i> {{ tutorial.date }}</p>
                    <p class="card-text space-icon"><i class="far fa-user"></i> {{ tutorial.author }}</p>
                    <!--<p class="card-text"><i class="fas fa-code-branch"></i> 4.0+</p>
