<!DOCTYPE html>
<html>
<head>
  {{> ga-tag-head}}
  {{> hs-tag}}
  
  <meta charset="utf-8">
  <meta name="description" content="{{#if social-description}}{{ social-description }}{{else}}The Truffle suite of tools make dapp development easier and more consistent.{{/if}}">
  {{#if keywords}}<meta name="keywords" content="{{ keywords }}">{{/if}}
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <!-- Favicon -->
  <link rel="icon" href="/img/favicons/favicon.ico">

  <!-- Add to homescreen for Chrome on Android -->
  <meta name="mobile-web-app-capable" content="yes">
  <meta name="application-name" content="Truffle Suite">
  <link rel="icon" sizes="192x192" href="/img/favicons/chrome-touch-icon-192x192.png">

  <!-- Add to homescreen for Safari on iOS -->
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black">
  <meta name="apple-mobile-web-app-title" content="Truffle Suite">
  <link rel="apple-touch-icon" href="/img/favicons/apple-touch-icon.png">

  <!-- Tile icon for Win8/10 (144x144 + tile color) -->
  <meta name="msapplication-TileImage" content="img/favicons/ms-touch-icon-144x144-precomposed.png">
  <meta name="msapplication-TileColor" content="#3372DF">

  <!-- Facebook OpenGraph -->
  <meta property="og:site_name" content="Truffle Suite" />
  <meta property="og:description" content="{{#if social-description}}{{ social-description }}{{else}}The Truffle suite of tools make dapp development easier and more consistent.{{/if}}" />
  <meta property="og:title" content="Events | Truffle Suite" />
  <meta property="og:url" content="{{getPageUrl path}}" />
  <meta property="og:type" content="article" />
  <meta property="og:image" content="https://truffleframework.com/img/favicons/truffle-share.png" />

  <!-- Twitter Card -->
  <meta name="twitter:card" content="summary" />
  <meta name="twitter:site" content="Truffle Suite" />
  <meta name="twitter:title" content="Events | Truffle Suite" />
  <meta name="twitter:description" content="{{#if social-description}}{{ social-description }}{{else}}The Truffle suite of tools make dapp development easier and more consistent.{{/if}}" />
  <meta name="twitter:creator" content="Truffle Suite" />
  <meta name="twitter:domain" content="trufflesuite.com" />
  <meta name="twitter:image:src" content="https://truffleframework.com/img/favicons/truffle-share.png" />
  
  <meta name="google-site-verification" content="BSgPkMHzw7IxJTpEElNfD8ZZYPzXgOQiTVPzAxAG8-o" />
  
  <title>Events | Truffle Suite</title>

  <link href="https://fonts.googleapis.com/css?family=Grand+Hotel|Open+Sans|Oswald|Varela+Round|Roboto+Condensed|Roboto+Mono" rel="stylesheet">
  <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.12.0/css/all.css" integrity="sha384-ekOryaXPbeCpWQNxMwSWVvQ0+1VrStoPJq54shlYhR8HzQgig1v5fas6YgOqLoKz" crossorigin="anonymous">
  <link rel="stylesheet" href="/css/index.css">
</head>
<body>
  {{> ga-tag-body}}
  
  {{> navbar}}

  <main class="container container-heading">
    <div class="row">
      <div class="col">
        <h1>UPCOMING EVENTS</h1>
      </div>
    </div>
  </main>

  <main class="banner banner-light-cream pt-5 pb-5" role="main">
    <div class="container events-container">
      {{#each events}}
        {{#if this.current}}
          {{#if this.image}}
            <div class="row align-items-center post-row pb-5">
              <div class="col-md-3">
                <img src="{{ this.image }}" class="img-fluid post-thumbnail" />
              </div>
              <div class="col-md-9">
                <h2>{{ this.title }}</h2>
                <p class="mb-1"><i class="far fa-calendar-alt"></i> {{ this.start }} - {{ this.end }}</p>
                {{#if this.location}}
                  <p><i class="fas fa-map-marker-alt"></i> {{ this.location }}</p>
                {{/if}}
                {{#if this.excerpt}}
                  <p>{{ this.excerpt }}</p>
                {{/if}}
                {{#if this.youtube}}
                   <a class="btn btn-youtube" href="{{ this.youtube }}" target="_blank">ATTEND THE LIVE SESSION</a>
                {{/if}}
                {{#if this.eventbrite}}
                <a class="btn btn-eventbrite" href="{{ this.eventbrite }}" target="_blank">REGISTER HERE</a>
                {{/if}}

              </div>
            </div>
          {{else}}
            <div class="row post-row">
              <div class="col">
                <h2>{{ this.title }}</h2>
                <p class="mb-1"><i class="far fa-calendar-alt"></i> {{ this.start }} - {{ this.end }}</p>
                {{#if this.location}}
                  <p><i class="fas fa-map-marker-alt"></i> {{ this.location }}</p>
                {{/if}}
                {{#if this.excerpt}}
                  <p>{{ this.excerpt }}</p>
                {{/if}}
                <!-- <a class="btn btn-eventbrite" href="{{ this.eventbrite }}">READ MORE</a> -->
              </div>
            </div>
          {{/if}}
        {{/if}}        
      {{/each}}
    </div>
  </main>

  <main class="container container-heading_alt">
    <div class="row">
      <div class="col">
        <h1>PAST EVENTS</h1>
      </div>
    </div>
  </main>

  <main class="banner banner-light-cream pt-5 pb-5" role="main">
    <div class="container events-container">
      {{#each events}}
        {{#if this.past}}
          {{#if this.image}}
            <div class="row align-items-center post-row  pb-5">
              <div class="col-md-3">
                <img src="{{ this.image }}" class="img-fluid post-thumbnail" />
              </div>
              <div class="col-md-9">
                <h2>{{ this.title }}</h2>
                <p class="mb-1"><i class="far fa-calendar-alt"></i> {{ this.start }} - {{ this.end }}</p>
                {{#if this.location}}
                  <p><i class="fas fa-map-marker-alt"></i> {{ this.location }}</p>
                {{/if}}
                {{#if this.excerpt}}
                  <p>{{ this.excerpt }}</p>
                {{/if}}
                <a class="btn btn-youtube" href="{{ this.youtube }}" target="_blank">WATCH  NOW</a>
              </div>
            </div>
          {{else}}
            <div class="row post-row">
              <div class="col">
                <h2>{{ this.title }}</h2>
                <p class="mb-1"><i class="far fa-calendar-alt"></i> {{ this.start }} - {{ this.end }}</p>
                {{#if this.location}}
                  <p><i class="fas fa-map-marker-alt"></i> {{ this.location }}</p>
                {{/if}}
                {{#if this.excerpt}}
                  <p>{{ this.excerpt }}</p>
                {{/if}}
                <!-- <a class="btn btn-eventbrite" href="{{ this.eventbrite }}">READ MORE</a> -->
              </div>
            </div>
          {{/if}}
        {{/if}}          
      {{/each}}
    </div>
  </main>

  {{> footer}}

  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
  <script>window.jQuery || document.write('<script src="/js/vendor/jquery-3.2.1.slim.min.js"><\/script>')</script>
  <script src="/js/bootstrap.min.js"></script>
  <!-- Start of HubSpot Embed Code -->
  <script type="text/javascript" id="hs-script-loader" async defer src="//js.hs-scripts.com/5258293.js"></script>
  <!-- End of HubSpot Embed Code -->
</body>
</html>